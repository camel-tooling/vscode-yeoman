{
  "name": "yo",
  "version": "0.9.11",
  "description": "Scaffold projects using Yeoman.",
  "license": "MIT",
  "publisher": "camel-tooling",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/camel-tooling/vscode-yeoman"
  },
  "bugs": {
    "url": "https://github.com/camel-tooling/vscode-yeoman/issues"
  },
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "https://github.com/SamVerschueren"
  },
  "maintainers": [
    {
      "name": "Brian Fitzpatrick",
      "email": "bfitzpat@redhat.com",
      "url": "http://redhat.com"
    }
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.29.1"
  },
  "activationEvents": [
    "onCommand:yeoman.yeoman",
    "onCommand:yeoman.yo"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "yeoman.yeoman",
        "title": "Generate Yeoman",
        "category": "Yeoman"
      },
      {
        "command": "yeoman.yo",
        "title": "Yo",
        "category": "Yeoman"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "yeoman.yeoman",
          "group": "navigation@50",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "clean": "rm -rf out || rmdir out /s /q",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run clean && npm run compile && node ./node_modules/vscode/bin/test",
    "update-deps": "npm update",
    "tslint": "gulp tslint"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.158",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.27",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.4",
    "chai": "^4.2.0",
    "generator-generator": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-tslint": "^8.1.4",
    "mocha": "^8.1.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "array-uniq": "^2.1.0",
    "diff": "^4.0.2",
    "elegant-spinner": "^2.0.0",
    "error-ex": "^1.3.2",
    "figures": "^3.2.0",
    "fix-path": "^3.0.0",
    "is-fn": "^2.0.0",
    "lodash": "^4.17.19",
    "opn": "^6.0.0",
    "read-pkg-up": "^7.0.1",
    "run-async": "^2.4.1",
    "semver": "^7.3.2",
    "strip-ansi": "^6.0.0",
    "yeoman-environment": "^2.10.3"
  }
}
