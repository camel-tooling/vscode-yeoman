{
  "name": "yo",
  "version": "0.9.8",
  "description": "Scaffold projects using Yeoman.",
  "license": "MIT",
  "publisher": "camel-tooling",
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/camel-tooling/vscode-yeoman"
  },
  "bugs": {
    "url": "https://github.com/camel-tooling/vscode-yeoman/issues"
  },
  "author": {
    "name": "Sam Verschueren",
    "email": "sam.verschueren@gmail.com",
    "url": "https://github.com/SamVerschueren"
  },
  "maintainers": [
    {
      "name": "Brian Fitzpatrick",
      "email": "bfitzpat@redhat.com",
      "url": "http://redhat.com"
    }
  ],
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.29.1"
  },
  "activationEvents": [
    "onCommand:yeoman.yeoman",
    "onCommand:yeoman.yo"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "yeoman.yeoman",
        "title": "yeoman"
      },
      {
        "command": "yeoman.yo",
        "title": "yo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "clean": "rm -rf out || rmdir out /s /q",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run clean && npm run compile && node ./node_modules/vscode/bin/test",
    "update-deps": "npm update",
    "tslint": "gulp tslint"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash": "^4.14.134",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.8",
    "@types/sinon": "^7.0.13",
    "@types/sinon-chai": "^3.2.2",
    "chai": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-tslint": "^8.1.4",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "array-uniq": "^2.1.0",
    "diff": "^4.0.1",
    "elegant-spinner": "^2.0.0",
    "error-ex": "^1.3.2",
    "figures": "^3.0.0",
    "fix-path": "^2.1.0",
    "is-fn": "^2.0.0",
    "lodash": "^4.17.11",
    "opn": "^6.0.0",
    "read-pkg-up": "^6.0.0",
    "run-async": "^2.3.0",
    "semver": "^6.1.1",
    "strip-ansi": "^5.2.0",
    "yeoman-environment": "^2.3.4"
  }
}
